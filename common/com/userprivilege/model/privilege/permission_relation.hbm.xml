<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 每个class对应一个持久化对象 -->
<hibernate-mapping>
	<class name="com.userprivilege.model.privilege.Permission_relation" table="permission_relation">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<property name="permid" column="permid" not-null="true" type="integer" />
		<property name="subpermid" column="subpermid" not-null="true"
			type="integer" />
	</class>
</hibernate-mapping>