<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 每个class对应一个持久化对象 -->
<hibernate-mapping>
	<class name="com.user.model.UserGroup_Relation" table="UserGroup_Relation">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<many-to-one name="groupid" column="groupid"
			class="com.user.model.UserGroup" />
		<many-to-one name="subgroupid" column="subgroupid"
			class="com.user.model.UserGroup" />
	</class>
</hibernate-mapping>