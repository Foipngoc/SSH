<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 每个class对应一个持久化对象 -->
<hibernate-mapping>
	<class name="com.privilege.model.Permission" table="permission">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="permname" column="permname"></property>
		<property name="permtype" column="permtype"></property>
		<property name="permdesc" column="permdesc"></property>
	</class>
</hibernate-mapping>