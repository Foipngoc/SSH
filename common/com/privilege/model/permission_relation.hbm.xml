<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 每个class对应一个持久化对象 -->
<hibernate-mapping>
	<class name="com.privilege.model.Permission_relation" table="permission_relation">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<many-to-one name="permid" column="permid" not-null="true"
			class="com.privilege.model.Permission"></many-to-one>
			<many-to-one name="subpermid" column="subpermid" not-null="true"
			class="com.privilege.model.Permission"></many-to-one>
	</class>
</hibernate-mapping>