<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 每个class对应一个持久化对象 -->
<hibernate-mapping>
	<class name="com.log.model.LogInfo" table="loginfo">
		<id column="id" name="id" type="integer">
			<generator class="native" />
		</id>
		<property name="logtype" column="logtype" type="integer"
			not-null="true"></property>
		<property name="loguser" column="loguser" type="string"
			length="20"></property>
		<property name="logtitle" column="logtitle" type="string"
			length="64"></property>
		<property name="logcontent" column="logcontent" type="string"
			length="255"></property>
		<property name="logtime" column="logtime" type="timestamp"
			not-null="true"></property>
		<property name="logstatus" column="logstatus" type="integer"
			not-null="true"></property>
	</class>
</hibernate-mapping>
