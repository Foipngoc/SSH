<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="loginfo" extends="cement-interceptor" namespace="/loginfo">
		<action name="addlog" class="com.log.action.LogInfoAction"
			method="addLogInfo">
			<result name="success" type="json" />
		</action>
		<action name="dellog" class="com.log.action.LogInfoAction"
			method="delLogInfo">
			<result name="success" type="json" />
		</action>
		<action name="querylogs" class="com.log.action.LogInfoAction"
			method="queryLogInfos">
			<result name="success" type="json" />
		</action>
		<action name="querylogsbytype" class="com.log.action.LogInfoAction"
			method="queryLogInfosByType">
			<result name="success" type="json" />
		</action>
		<action name="querylog" class="com.log.action.LogInfoAction"
			method="queryLogInfo">
			<result name="success" type="json" />
		</action>
	</package>
</struts>